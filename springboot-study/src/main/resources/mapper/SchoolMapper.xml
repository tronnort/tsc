<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tron.web.mybatis.SchoolMapper">
    <resultMap id="schoolMap" type="com.tron.web.mybatis.School">
        <id property="id" column="id" />
        <result property="name" column="name"/>
        <result property="address" column="address"/>
        <collection property="schoolClass" select="com.tron.web.mybatis.SchoolClassMapper.getSchoolClassBySchoolId" column="id"/>
    </resultMap>
</mapper>
